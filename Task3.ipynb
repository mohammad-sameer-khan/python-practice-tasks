   "source": [
    "def create(n):\n",
    "    dicts = {}\n",
    "    n+=1\n",
    "    keys = range(1,n)\n",
    "    for i in keys:\n",
    "        dicts[i] = i*i\n",
    "    return dicts"
   ]
  },
